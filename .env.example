# Kalshi API environment
# true = demo API (demo-api.kalshi.com)
# false = production API (trading-api.kalshi.com)
KALSHI_USE_DEMO=true

# Market series to watch
KALSHI_EVENT_TICKER_PREFIX=KXBTC15M

# Polling interval in seconds
POLL_SECONDS=5

# Paper stake per trade in USD
MOCK_BUY_USD=5

# Output CSV path for mock trades
MOCK_TRADES_CSV_PATH=data/mock_trades.csv

# Lookback window for BTC momentum strategy (seconds)
MOMENTUM_WINDOW_SECONDS=60
